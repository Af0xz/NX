{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "dev:i": {
      "cache": false
    },
    "dev:rc": {
      "cache": false
    },
    "dev:ig": {
      "outputs": []
    },
    "check:tsc": {
      "outputs": [],
      "cache": false
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "clean": {
      "outputs": [],
      "cache": false
    }
  }
}
